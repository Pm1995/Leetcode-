#print spiral pattern 2d
matrix=[[2,4,6,8],[5,9,12,16],[2,11,5,9],[3,2,1,8]]

T=0;B=3;L=0;R=3;dir=0
while T<=B and L<=R:
	if dir==0:
		for i in range(L,R+1):
			print(matrix[T][i])
		T=T+1
		dir=1
	elif dir==1:
		for i in range(T,B+1):
			print(matrix[i][R])
		R=R-1
		dir=2
	elif dir==2:
		for i in range(R,L-1,-1):
			print(matrix[B][i])
		B=B-1
		dir=3
	elif dir==3:
		for i in range(B,T-1,-1):
			print(matrix[i][L])
		L=L+1
		dir=0







